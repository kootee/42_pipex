{
	"version": "0.2.0",
	"configurations": [

		{
			"name": "C Launch",
            "type": "cppdbg",
            "request": "launch",
			"program": "${workspaceFolder}/pipex",
			"cwd": "${workspaceFolder}",
			"envFile": "${workspaceFolder}/env_file.txt", 
            "externalConsole": false,
            "MIMode": "lldb",
			"args": ["input", "grep Hello", "ls -l src/", "output"],
			"stopAtEntry": true,
            "serverLaunchTimeout": 5000,
            "filterStderr": false,
            "filterStdout": false
/*             "logging": {
                "moduleLoad": false,
                "trace": true,
                "traceResponse": true
            }, */
		}
	]
}